


<h4> Share search </h4>

<form>
    <input type="text" id='ASXInput' placeholder="search Shares">
</form>
<input type="submit" value="getASX" class="select3"/>

<table class="table" id="fundTable">
<tr>
        <th scope="col">  &#10003;</th>
        <th scope="col"> ASX code </th>
        <th scope="col"> Name </th>
        <th scope="col"> Category</th>
    </tr> 
<% ASX.forEach(function(ASX){ %>
<tbody id="ASXTable">
    <tr>
        <th scope="col">  <input type="checkbox" name='ASXcheck'></th>
        <th scope="col" class="code1"> <%= ASX.ASXcode %> </th>
        <th scope="col" class="share-name"> <%= ASX.ListedInvestmentName %> </th>
        <th scope="col" class="category"> <%= ASX.Category%> </th>
    </tr> 
</tbody>
<%})%>
</table>




</div>

