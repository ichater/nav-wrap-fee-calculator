<div class="container">
<h1> test data </h1>

<form id="search-SMA">
    <input type="text" id='myInput' onkeyup='searchTable()' placeholder="search SMAs">
</form>

<table class="table" id="fundTable">
<% SMA.forEach(function(SMA){ %>
<tbody id="myTable">
    <tr>
        <th scope="col"> <input type="checkbox"> <%= SMA.Name %> </th>
        <th scope="col"> <%= SMA.APIR %> </th>
        <th scope="col"> <%= SMA.Nab%> </th>
        <th scope="col"> <%= SMA.MER%> </th>
    </tr> 
</tbody>
<%})%>
</table>




</div>

